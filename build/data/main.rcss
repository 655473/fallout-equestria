button:active
{
  //background-image:     hud.png 204px 88px 373px 115px;
  background-image: revelation-mod/menu_button.png 0xp 49px 247px 96px;
}

button.button_menu
{
  display: inline-block;

  width: 185px;
  height: 28px;

  vertical-align: -18px;

  padding-left: 60px;

  margin: 5px 5px 5px 5px;

  font-size: 18;
  text-align: center;
  tab-index: auto;

  background-decorator: image;
  background-image: hud.png 0px 121px 185px 148px;
}

button.button_menu:focus,
{
    font-weight: bold;
    
    shadow-font-effect: shadow;
    shadow-offset: 1px 1px;
    shadow-color: black;
}

button.button_menu:hover,
{
	background-image: hud.png 0px 149px 185px 175px;
}

button.button_menu:active,
{
	background-image: hud.png 0px 176px 185px 203px;
}

