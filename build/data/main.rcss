/*######################################################
          SCROLLBAR
######################################################*/
scrollbarvertical
{
  width: 28px;
}

scrollbarvertical slidertrack
{
  background-decorator: tiled-vertical;
  background-top-image: hud.png 469px 119px 497px 148px;
  background-center-image: hud.png 469px 149px 497px 225px;
  background-bottom-image: hud.png 469px 226px 497px 255px;
}

scrollbarvertical sliderbar
{
  margin: auto;
  background-decorator: tiled-vertical;
  background-top-image: hud.png 440px 0px 469px 30px;
  background-center-image: hud.png 440px 31px 469px 86px;
  background-bottom-image: hud.png 440px 87px 469px 117px;
}

scrollbarvertical sliderbar:active
{
  background-top-image-s: 470px 498px;
  background-center-image-s: 470px 498px;
  background-bottom-image-s: 470px 498px;
}

scrollbarvertical sliderarrowdec
{
  width: 27px;
  height: 24px;
  icon-decorator: image;
  icon-image: hud.png 404px 122px 434px 152px;
}

scrollbarvertical sliderarrowdec:active,
scrollbarvertical sliderarrowinc:active
{
 icon-image-t: 152px 182px;
}

scrollbarvertical sliderarrowinc
{
  width: 27px;
  height: 24px;
  icon-decorator: image;
  icon-image: hud.png 374px 122px 404px 152px;
}


