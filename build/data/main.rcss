/*##############################################################
                     Generic id decorators
##############################################################*/
body
{
  font-family: Delicious;
  font-weight: normal;
  font-style: normal;
  font-size: 15;
}

div#window
{
  height: 100%;
  padding: 10px 15px;
  background-decorator: tiled-box;
  background-top-left-image:     invader.tga           0px   0px  30px  30px;
  background-top-right-image:    invader.tga         202px   0px 231px  30px;
  background-top-image:          invader.tga repeat-truncate  30px   0px 201px  30px;
  background-bottom-left-image:  invader.tga           0px  91px  30px 119px;
  background-bottom-right-image: invader.tga         202px  91px 231px 119px;
  background-bottom-image:       invader.tga repeat-truncate  30px  91px 201px 119px;
  background-left-image:         invader.tga repeat-truncate   0px  30px  30px  90px;
  background-right-image:        invader.tga repeat-truncate 202px  30px 231px  90px;
  background-center-image:       invader.tga repeat-truncate  30px  30px 201px  90px;
}

.inventory-item-quantity
{
  font-family: JH_Fallout;
  font-size:   8px;
  margin-left: -15px;
  top: 0px;
}

div#content
{
  z-index: 2;
  overflow: hidden auto;
  text-align: center;
}

select,
dataselect
{
  margin-left: 20px;
}
.title{
	text-align: center;	
}

/*################################################
         BUTTONS
################################################*/
/*####### SIMPLE ######*/
button.simple-button
{
  display: inline-block;
  width: 79;
  height: 35;
  background-decorator: image;
  background-image: button.png;
}

button.simple-button:active
{
  background-decorator: image;
  background-image: button-pressed.png;
}
/*####### FRANCIS ######*/
.francis
{
  display: inline-block;
  font-size: 10px;
  text-align: center;
  tab-index: auto;
  width: 60px;
  height: 20px;
  background-decorator: image;
  background-image: hud.png 0px 0px 125px 39px;
}

.francis:focus
{
  font-weight: bold;
  shadow-font-effect: shadow;
  shadow-offset: 1px 1px;
  shadow-color: black;
}

.francis:hover
{
  background-image: hud.png 0px 40px 125px 79px;
}

button.francis:active
{
  background-image: hud.png 0px 80px 125px 119px;
}
/*####### UNIVERSAL ######*/
.universal_button
{
  display: inline-block;
  height: 47px;
  width: 247px;
  text-align: center;
  tab-index:  auto;
  font-family:  Fallout;
  font-size:    20px;
  background-decorator: image;
  //background-image:     hud.png 204px 32px 373px 58px;
  background-image: revelation-mod/menu_button.png 0px  0px 247px 47px;
}

/*button:hover
{
  background-image:     hud.png 204px 59px 373px 87px;
}*/

.universal_button:active
{
  //background-image:     hud.png 204px 88px 373px 115px;
  background-image: revelation-mod/menu_button.png 0px 49px 247px 96px;
}

/*####### BUTTON_MENU ######*/
