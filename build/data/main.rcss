	background-top-image:    invader.tga         70px 320px 97px 322px;
	background-center-image: invader.tga stretch 70px 322px 97px 323px;
	background-bottom-image: invader.tga         70px 324px 97px 325px;
}

.console-bg scrollbarvertical sliderbar,
{
	margin-left: 0px;
	width:       10px;
	min-height:  46px;

	background-decorator:    tiled-vertical;
	background-top-image:    invader.tga         56px 273px 79px 296px;
	background-center-image: invader.tga stretch 56px 296px 79px 296px;
	background-bottom-image: invader.tga         56px 296px 79px 319px;
}

