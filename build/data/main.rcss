{
	background-top-image-s: 80px 103px;
	background-center-image-s: 80px 103px;
	background-bottom-image-s: 80px 103px;
}

.console-bg scrollbarvertical sliderbar:active
{
	background-top-image-s: 104px 127px;
	background-center-image-s: 104px 127px;
	background-bottom-image-s: 104px 127px;
}

.console-bg scrollbarvertical sliderarrowdec,
{
	width: 10px;
	height: 10px;
	
	icon-decorator: image;
	icon-image: invader.tga 0px 273px 27px 297px;
}

.console-bg scrollbarvertical sliderarrowinc,
{
	width: 10px;
	height: 10px;
	
	icon-decorator: image;
	icon-image: invader.tga 28px 273px 55px 297px;
}

.console-bg scrollbarvertical sliderarrowdec:hover,
.console-bg scrollbarvertical sliderarrowinc:hover
{
	icon-image-t: 177px 201px;
}

.console-bg scrollbarvertical sliderarrowdec:active,
.console-bg scrollbarvertical sliderarrowinc:active
{
	icon-image-t: 202px 226px;
}


